                  AnchorPane.rightAnchor="400.0" AnchorPane.topAnchor="100.0">
        </GridPane>
        <VBox fx:id="vBoxShips" alignment="CENTER" layoutX="893.0" layoutY="103.0" prefHeight="524.0" prefWidth="300.0"
              spacing="10.0" AnchorPane.bottomAnchor="123.0" AnchorPane.rightAnchor="50.0"
              AnchorPane.topAnchor="103.0"/>
        <Button fx:id="buttonBack" layoutX="100.0" layoutY="668.0" mnemonicParsing="false"
