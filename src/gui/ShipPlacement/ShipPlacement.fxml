<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<?import java.net.URL?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="750.0" prefWidth="1200.0" styleClass="anchorPane"
            stylesheets="@../generalApplication%20Design.css" xmlns="http://javafx.com/javafx/10.0.2-internal">
    <children>
        <GridPane fx:id="dataGridBattleship" hgap="2.0" layoutX="115.0" layoutY="105.0" prefHeight="489.0"
