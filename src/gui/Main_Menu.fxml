<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import java.net.URL?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:id="anchorPane" prefHeight="750.0" prefWidth="1200.0"
            styleClass="anchorPaneMainMenu" stylesheets="@generalApplication%20Design.css"
            xmlns="http://javafx.com/javafx/10.0.2-internal">
   <children>
      <VBox alignment="CENTER" layoutX="162.0" layoutY="230.0" minHeight="-Infinity" minWidth="-Infinity"
            prefHeight="256.0" prefWidth="220.0" spacing="20.0" styleClass="vBoxTask" AnchorPane.leftAnchor="110.0"
            AnchorPane.topAnchor="230.0">
         <children>
            <Button fx:id="buttonNewGame" mnemonicParsing="false" onMouseClicked="#createNewGame" prefHeight="25.0"
                    prefWidth="200.0" styleClass="btnMenu" stylesheets="@generalApplication%20Design.css"
                    text="Neues Spiel" VBox.vgrow="ALWAYS"/>
            <Button fx:id="buttonLoadGame" mnemonicParsing="false" onMouseClicked="#loadExistingGame" prefHeight="25.0"
                    prefWidth="200.0" styleClass="btnMenu" text="Spiel laden"/>
            <Button fx:id="buttonSettings" mnemonicParsing="false" onMouseClicked="#loadSettings" prefHeight="41.0"
                    prefWidth="200.0" styleClass="btnMenu" text="Einstellungen"/>
            <Button fx:id="buttonExit" mnemonicParsing="false" onMouseClicked="#closeApplication" prefHeight="25.0"
                    prefWidth="200.0" styleClass="btnMenu" text="Beenden"/>
         </children>
         <padding>
            <Insets bottom="15.0" left="15.0" right="15.0" top="15.0"/>
         </padding>
      </VBox>
      <Label alignment="CENTER" layoutX="200.0" layoutY="74.0" prefHeight="41.0" prefWidth="220.0" styleClass="label-h1"
             text="Schiffe Versenken" AnchorPane.leftAnchor="200.0" AnchorPane.topAnchor="74.0"/>
   </children>
   <stylesheets>
      <URL value="@/styles/general.css"/>
      <URL value="@/styles/mainMenu.css"/>
   </stylesheets>
</AnchorPane>
